<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>

  <%= stylesheet_link_tag 'application.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/style.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/font-awesome.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/owl.carousel.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/owl.theme.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/slick.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/slick-theme.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/starw.css', media: 'all' %>
  <%= stylesheet_link_tag 'saprol/woocommerce.css', media: 'all' %>



  <%= javascript_include_tag 'application.js' %>
  <%= javascript_include_tag 'saprol/jquery-1.12.0.min.js'%>
  <%= javascript_include_tag 'saprol/custom.js'%>
  <%= javascript_include_tag 'saprol/imagesloaded.pkgd.min.js' %>
  <%= javascript_include_tag 'saprol/bootstrap.min.js' %>
  <%= javascript_include_tag 'saprol/jquery.fitvids.min.js' %>
  <%= javascript_include_tag 'saprol/jquery.matchHeight-min.js' %>
  <%= javascript_include_tag 'saprol/masonry.pkgd.min.js' %>
  <%= javascript_include_tag 'saprol/owl.carousel.min.js' %>
  <%= javascript_include_tag 'saprol/slick.min.js'%>
  <%= javascript_include_tag 'saprol/superclick.min.js' %>
  <%= javascript_include_tag 'saprol/superfish.min.js'%>

  <%= javascript_include_tag 'http://maps.google.com/maps/api/js?sensor=true' %>
  <%= javascript_include_tag 'lib/gmaps.js' %>
  <%= javascript_include_tag 'lib/cycle.js'%>
  <%= javascript_include_tag 'lib/jquery.browser.js' %>

  <%= csrf_meta_tags %>
</head>
<body>
<div class="main-container">
  <%= render 'layouts/header' %>
  <div class="main-content">
    <% if notice %>
        <p class="alert alert-success fade-out"><%= notice %></p>
    <% end %>
    <% if alert %>
        <p class="alert alert-danger fade-out"><%= alert %></p>
    <% end %>
    <%= yield %>
  </div>
  <%= render 'layouts/footer' %>
</div>

</body>
</html>

