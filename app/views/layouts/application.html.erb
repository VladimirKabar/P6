<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>

  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= stylesheet_link_tag 'style.css', media: 'all' %>


  <%= javascript_include_tag 'js/jquery-1.12.0.min', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/bootstrap.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/jquery.matchHeight-min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/owl.carousel.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/masonry.pkgd.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/imagesloaded.pkgd.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/superclick.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/superfish.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'http://maps.google.com/maps/api/js?sensor=true', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/gmaps', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/jquery.fitvids.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/slick.min.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'js/custom.js', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jquery.browser', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'cycle.js', media: 'all', 'data-turbolinks-track' => true %>





  <%= csrf_meta_tags %>
</head>
<body>
<div class="main-container">
  <%= render 'layouts/header' %>
  <div class="main-content">
    <% if notice %>
        <p class="alert alert-success"><%= notice %></p>
    <% end %>
    <% if alert %>
        <p class="alert alert-danger"><%= alert %></p>
    <% end %>
    <%= yield %>
  </div>
  <%= render 'layouts/footer' %>
</div>

</body>
</html>

